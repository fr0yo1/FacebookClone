@model FacebookClone.Models.SearchViewModel
@using FacebookClone.Models
@{
    ViewBag.Title = "SearchView";
}
<br />
<div class="container">
    <h2>Groups:</h2>
    @if (@Model.groupsResult == null)
    {
        <p>No result</p>
    }
    else
    {
        foreach (var genericItem in @Model.groupsResult)
        {
         @Html.Partial("GenericItemPartialView", genericItem)
        }
    }
    <h2>Users:</h2>
    @foreach (var genericItem in @Model.usersResult)
    {
        @Html.Partial("GenericItemPartialView", genericItem)
    }
    @if (!@Model.usersResult.Any())
    {
        <p>No result</p>
    }
</div>

